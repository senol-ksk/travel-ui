{
  "name": "travel-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev-https": "next dev --experimental-https",
    "dev-https-inspect": "NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_OPTIONS='--inspect' next dev --experimental-https",
    "build": "next build --turbopack",
    "start": "next start",
    "prepare": "husky && pnpm run theme",
    "test:watch": "vitest",
    "test:staged": "vitest --run",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "tsc --noEmit",
    "test": "pnpm run prettier:check && pnpm run lint && pnpm run typecheck && pnpm run test:staged",
    "prettier": "prettier --write **/**/*.{ts,tsx,css}",
    "prettier:check": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "theme": "pnpm dlx tailwind-preset-mantine src/styles/mantine/index.ts -o src/styles/theme.css",
    "email:nodemon": "nodemon --watch src/emails --ext ts,tsx,js,json --exec \"email dev --port 3001 --dir src/emails\"",
    "email": "email dev --dir ./src/emails/ --port 3001"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@mantine/carousel": "^8.2.7",
    "@mantine/core": "^8.2.7",
    "@mantine/dates": "^8.2.7",
    "@mantine/hooks": "^8.2.7",
    "@mantine/modals": "^8.2.7",
    "@mantine/notifications": "^8.2.7",
    "@mantine/nprogress": "^8.2.7",
    "@number-flow/react": "^0.5.10",
    "@react-email/components": "0.5.1",
    "@tanstack/react-query": "^5.85.5",
    "@vis.gl/react-google-maps": "^1.5.5",
    "axios": "^1.11.0",
    "card-validator": "^10.0.3",
    "cleave-zen": "^0.0.17",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "embla-carousel": "^8.6.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "i18next": "^25.4.2",
    "intl-tel-input": "^25.5.2",
    "md5": "^2.3.0",
    "next": "15.5.0",
    "next-auth": "5.0.0-beta.25",
    "next-view-transitions": "^0.3.4",
    "nuqs": "^2.5.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "react-virtuoso": "^4.14.0",
    "resend": "^6.0.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "tailwind-preset-mantine": "^2.1.0",
    "validator": "^13.15.15",
    "zod": "^3.25.76",
    "zod-i18n-map": "^2.27.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@react-email/preview-server": "^4.2.8",
    "@tailwindcss/postcss": "^4.1.12",
    "@tanstack/eslint-plugin-query": "^5.83.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/md5": "^2.3.5",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.8",
    "@types/validator": "^13.15.2",
    "@vitejs/plugin-react": "^5.0.1",
    "browser-sync": "^3.0.4",
    "eslint": "9.34.0",
    "eslint-config-mantine": "^4.0.3",
    "eslint-config-next": "15.5.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react-email": "4.2.8",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
